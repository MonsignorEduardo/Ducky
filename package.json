{
    "name": "ducky",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "libs/*"
    ],
    "scripts": {
        "db-generate": "turbo db-generate",
        "db-push": "turbo db-push",
        "build": "turbo build",
        "dev": "turbo dev --parallel",
        "clean:workspaces": "turbo clean",
        "clean": "rm -rf ./node_modules ./.turbo ./.vercel",
        "lint": "turbo lint"
    },
    "devDependencies": {
        "prettier": "latest",
        "turbo": "^1.6.1",
        "typescript": "^4.8.4"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "packageManager": "pnpm@7.13.2"
}
