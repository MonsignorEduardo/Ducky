{
    "name": "ducky",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "build": "turbo run build",
        "build:telegram": "turbo run build --filter @ducky/telegram",
        "build:web-ui": "turbo run build --filter @ducky/web-ui",
        "dev": "turbo run dev --parallel",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "clean": "rimraf ./**/node_modules",
        "start:telegram": "turbo run start --filter @ducky/telegram"
    },
    "dependencies": {
        "rimraf": "^3.0.2"
    },
    "devDependencies": {
        "prettier": "latest",
        "turbo": "^1.6.1",
        "typescript": "^4.8.4"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "packageManager": "pnpm@7.11.0"
}
