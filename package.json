{
    "name": "ducky",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "build:telegram-bot": "turbo run build --filter @ducky/telegram-bot",
        "build:web-ui": "turbo run build --filter @ducky/web-ui",
        "dev": "turbo run dev --parallel",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "postinstall": "prisma generate",
        "db:push": "prisma db push",
        "db:seed": "prisma db seed"
    },
    "dependencies": {
        "@prisma/client": "^4.3.1"
    },
    "devDependencies": {
        "prettier": "latest",
        "prisma": "^4.3.1",
        "turbo": "^1.5.5",
        "typescript": "^4.8.4"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "packageManager": "pnpm@7.11.0"
}
