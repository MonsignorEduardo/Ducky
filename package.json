{
  "name": "my-turborepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "postinstall": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed"
  },
  "dependencies": {
    "@prisma/client": "^4.3.1"
  },
  "devDependencies": {
    "prettier": "latest",
    "turbo": "latest",
    "prisma": "^4.3.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.11.0"
}
