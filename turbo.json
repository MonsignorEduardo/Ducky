{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": ["db-generate", "^build"],
            "outputs": [".next/**", "build/**"],
            "cache": true
        },
        "lint": {
            "outputs": []
        },
        "dev": {
            "cache": false
        },
        "start": {
            "cache": false
        },
        "db-generate": {
            "outputs": ["../../node_modules/.prisma/client"],
            "cache": true
        },
        "db-push": {
            "cache": false
        },
        "clean": {
            "cache": false
        }
    }
}
